<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="studentsChatVM" type="com.munirasapplication.app.modules.studentschat.data.viewmodel.StudentsChatVM"/>
    </data>
    <LinearLayout android:gravity="end"
        android:id="@+id/linearStudentschat"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_17pxv"
        android:layout_marginEnd="@dimen/_19pxh"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <LinearLayout android:id="@+id/linearColumnunsplashptrhfm"
            android:layout_height="0dp" android:layout_weight="1.0"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackunsplashptrhfm"
                android:layout_height="@dimen/_187pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageUnsplashpTrhfm"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_90pxh"
                    android:layout_marginEnd="@dimen/_88pxh"
                    android:layout_marginStart="@dimen/_88pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_92pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_20pxh}"
                    imageRes="@{@drawable/img_unsplashptrhfm}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_unsplashptrhfm"/>
                <FrameLayout android:id="@+id/frameStacktime"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_187pxv" android:layout_width="match_parent">

                    <ImageView android:id="@+id/imageEllipseOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_187pxh"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="@dimen/_137pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_ellipse1_5}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse1_5"/>
                </FrameLayout>
            </FrameLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnsoremipsumdol"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_26pxv"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStylegray_100_edcornerRadius">
                <TextView android:gravity="start"
                    android:id="@+id/txtSoremipsumdol"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="match_parent"
                    android:text="@{studentsChatVM.studentsChatModel.txtSoremipsumdol}"
                    style="@style/txtPoppinsbold12_1" tools:text="@string/msg_sorem_ipsum_dol"/>
            </LinearLayout>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearRowlorem"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_6pxh"
                android:layout_marginStart="@dimen/_207pxh"
                android:layout_marginTop="@dimen/_25pxv"
                android:layout_width="wrap_content"
                android:orientation="horizontal" style="@style/groupStyledeep_purple_100cornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLorem"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_28pxv"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{studentsChatVM.studentsChatModel.txtLorem}"
                    style="@style/txtPoppinsbold12_1" tools:text="@string/lbl_lorem"/>
                <ImageView android:id="@+id/imageArrowdown"
                    android:layout_height="@dimen/_19pxh"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_46pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="@dimen/_15pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowdown"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowdown"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnboremipsumdol"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_29pxv"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStylegray_100_ed1cornerRadius">
                <TextView android:gravity="start"
                    android:id="@+id/txtBoremipsumdol"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_32pxv"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent"
                    android:text="@{studentsChatVM.studentsChatModel.txtBoremipsumdol}"
                    style="@style/txtPoppinsbold12_1" tools:text="@string/msg_borem_ipsum_dol"/>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameStackvector"
                android:layout_gravity="start"
                android:layout_height="@dimen/_54pxv"
                android:layout_marginEnd="@dimen/_32pxh"
                android:layout_marginStart="@dimen/_32pxh"
                android:layout_marginTop="@dimen/_33pxv"
                android:layout_width="@dimen/_110pxh" style="@style/groupStylegray_101cornerRadius">
                <ImageView android:id="@+id/imageVector"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_7pxh"
                    android:layout_marginBottom="@dimen/_23pxv"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
            </FrameLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_2pxh"
            android:layout_marginStart="@dimen/_25pxh"
            android:layout_width="match_parent" style="@style/groupStylebluegray_100_51cornerRadius">
            <LinearLayout android:id="@+id/linearRowhome"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_11pxv"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageHome"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_home_gray_700"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_home_gray_700"/>
                <ImageView android:id="@+id/imageCalendar"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_calendar_gray_703"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_calendar_gray_703"/>
                <ImageView android:id="@+id/imageVolume"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_46pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_volume"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_volume"/>
                <ImageView android:id="@+id/imageUser"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginEnd="@dimen/_39pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
